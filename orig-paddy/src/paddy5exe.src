__PaddyDDMessage("set_defaults done, definitionfile: " . definitionfile,my_name)
IniRead, test, %Definitionfile%, Settings, debugmode,99
If ((include_debug) And (test <> 99))
{
IniRead, debugmode, %Definitionfile%, Settings, debugmode,0
IniRead, verbose_debug, %Definitionfile%, Settings, verbose_debug,0
IniRead, detailed_debug, %Definitionfile%, Settings, detailed_debug,0
IniWrite, %debugmode%, %A_ScriptDir%\debug_setting.ini, Debug, debugmode
IniWrite, %verbose_debug%, %A_ScriptDir%\debug_setting.ini, Debug, verbose_debug
IniWrite, %detailed_debug%, %A_ScriptDir%\debug_setting.ini, Debug, detailed_debug
IniDelete, %Definitionfile%, Settings, debugmode
IniDelete, %Definitionfile%, Settings, verbose_debug
IniDelete, %Definitionfile%, Settings, detailed_debug
If ((DEBUGMODE & 2) = 2)
FileCreateDir, %A_Desktop%\Paddy Log Files
}
set_region()
read_presets()
SetTimer, LR_active_Poller, 1000
SetTimer, poll_box_handles, 2000
menus_created := 0
If (FileExist(A_ScriptDir . "\paddy_registration.ini") And !FileExist(Paddy_settingsdir . "\paddy_registration.ini"))
{
FileCopy,%A_ScriptDir%\paddy_registration.ini,%Paddy_settingsdir%\paddy_registration.ini
FileDelete,%A_ScriptDir%\paddy_registration.ini
Loop
{
Sleep, 100
If !FileExist(A_ScriptDir . "\paddy_registration.ini")
Break
}
}
If IsFunc("registered_copy")
{
registered:=registered_copy(days_left)
If (registered<>2)
{
dFile_to_send:="[FAIL " . registered . " days:" . days_left . "]" . debug_file_to_send
SetTimer,send_registration_debug,-1000
show_splash()
}
Else
{
dfile_to_send:="[OK]" . debug_file_to_send
SetTimer,send_registration_debug,-1000
}
}
registerloop:
wait_for_registration:=1
If (WinExist("Paddy by Dorfl"))
{
SetTimer, registerloop, -300
Return
}
wait_for_registration:=0
SetTimer,send_registration_debug,-1000
IniRead, use_driverless_xkeys, %Definitionfile%, Settings, use_driverless_xkeys,0
If (use_driverless_xkeys)
{
IniRead, xkeys_type, %Definitionfile%, Settings, xkeys_type,XKeysPro
DLL := DllCall("LoadLibrary",Str,"PaddyDotNet.dll")
If !DLL
MsgBox, Could Not load XKeys DLL
COM_Init()
XkeysObject := COM_CreateObject("PaddyDotNet.XKeysDriver")
OnMessage(0x4004,"test_receiver")
If !XKeysobject
MsgBox, Error invoking the XKeys driver
Else
{
st:=COM_Invoke(XkeysObject, "ping","Test Com: success")
_Paddyddmessage(st,"[PADDY]")
er:=COM_Invoke(XkeysObject, "Initialize")
_Paddyddmessage("initializing XKeys returned: " . er,"[PADDY]")
If (er=102)
MsgBox, No supported XKeys Found
Else if (er>0)
MsgBox, Error Initializing the XKeys driver!`nError: %er%
Else
{
PID:=COM_Invoke(XkeysObject, "get_XKeys_PID")
If ((PID = 679) or (PID = 657))
xkeys_type:="XKeysPro"
Else if (PID = 1089)
xkeys_type:="XK-80"
Else if (PID = 1121)
xkeys_type:="XK-60"
IniWrite, %xkeys_type%, %Definitionfile%, Settings, xkeys_type
_Paddyddmessage("xkeys identified as: " . xkeys_type,"[PADDY]")
SetTimer,flashxkeylights,-100
IniRead, XKeys_Layer_2_list,  %Definitionfile%, XKeys, XKeys_Layer_2_list,52`,57
IniRead, XKeys_Layer_3_list,  %Definitionfile%, XKeys, XKeys_Layer_3_list,51`,58
IniRead, xkeys_toggle_are_sticky,  %Definitionfile%, XKeys, xkeys_toggle_are_sticky,0
COM_Invoke(XkeysObject, "SetupLayers", XKeys_Layer_2_list, XKeys_Layer_3_list)
COM_Invoke(XkeysObject, "make_toggle_sticky", xkeys_toggle_are_sticky)
OnMessage(0x4001,"XkeyNormal_Message_receiver")
OnMessage(0x4002,"Xkey_error_receiver")
}
}
}
Else
xkey_driver_pid :=0
IniRead, lock_numlock,  %Definitionfile%, Settings, lock_Numlock, 1
