auto_b:
{
Auto_white(1)
Return
}
Auto_black(k)
{
global need_slider_handles
WinActivate, ahk_class AgWinMainFrame
If (Process_number()<>12)
{
MsgBox,4096, Paddy Error, This only works for 2012 Process
Return
}
CoordMode, Pixel, Screen
CoordMode, Mouse, Screen
hist:="AfxWnd100u90"
ControlGetPos,cx,cy,cw,h,%hist%,ahk_class AgWinMainFrame
WinGetPos,wx,wy,w,h,ahk_class AgWinMainFrame
yt:=(wy + cy+10)
xbt:=(wx + cx+8)
If need_slider_handles
read_all_slider_handles()
paddy_slider:=8
r:= get_slider_handle_from_paddy_virtual_slider_number(paddy_slider, edit_handle)
If r
{
MsgBox,4096, Paddy Error, Couldn't do it, Not sure why, sorry
Return
}
slider_pos:=0
Set_Slider(paddy_slider, edit_handle, slider_pos, 0)
Sleep, 200
If clipping_is_good(xbt,yt)
old_slider_pos:=-100
Else
old_slider_pos:=100
Loop, 16
{
new_slider_pos:=Round((slider_pos+old_slider_pos)/2,0)
If (Abs(Abs(new_slider_pos)-Abs(slider_pos))<2)
Break
Set_Slider(paddy_slider, edit_handle, new_slider_pos, 0)
c:=clipping_is_good(xbt,yt)
If (c And (new_slider_pos<old_slider_pos))
{
old_slider_pos:=slider_pos
slider_pos:= new_slider_pos
}
If (c And (new_slider_pos>old_slider_pos))
slider_pos:= new_slider_pos
If (!c And (new_slider_pos<old_slider_pos))
slider_pos:= new_slider_pos
If (!c And (new_slider_pos>old_slider_pos))
{
old_slider_pos:=slider_pos
slider_pos:= new_slider_pos
}
}
finish_slider_event()
Return
}
