Sleep,100
}
ControlGet, out, Visible,,, ahk_id %keyword_panel_handle%
If !out
{
s := get_panel_status()
If !(s&1)
{
open_right_panel(1)
Sleep,400
}
ControlGet, out, Visible,,, ahk_id %keyword_panel_handle%
If !out
{
Send,^k
Sleep, 300
}
ControlGet, out, Visible,,, ahk_id %keyword_panel_handle%
If !out
Sleep, 300
}
ControlGetFocus,out,ahk_class AgWinMainFrame
If (out<>tag_add_box)
{
OutputDebug, getting tagbox
ControlClick,,ahk_id %tag_add_box_handle%
If !tag_box_selected()
Return
}
Send ,%keyword%
Sleep, 20
Send {Enter}
Sleep, 20
SetTimer,kill_tab_box,-600
Return
}
