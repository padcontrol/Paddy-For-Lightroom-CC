Loop, 16
{
channel := A_Index
Loop, 127
{
f_number := Midi_%channel%_%A_Index%_Pick
f:= function_call%f_number%
paddy_slider2 := SubStr(f,4)
If ((f_number>= 1326) And (f_number<=1559))
{
midi_count_for_paddy_slider_%paddy_slider2%++
slider_%paddy_slider2%_is_mapped_to_midi:=1
count2 := midi_count_for_paddy_slider_%paddy_slider2%
midi_control_%count2%_for_paddy_slider_%paddy_slider2% :=A_Index
midi_channel_%count2%_for_paddy_slider_%paddy_slider2% := channel
}
}
}
Loop, 16
{
f_number := pitch_%A_Index%_Pick
f:= function_call%f_number%
paddy_slider2 := SubStr(f,4)
If ((f_number>= 1326) And (f_number<=1559))
{
mapped_to_pitch_%paddy_slider2%  := A_Index
slider_%paddy_slider2%_is_mapped_to_midi:=1
}
}
Loop, 4
{
subpage:=A_Index
Loop, 127
{
f_number:=Nocturn_cc_%A_Index%_subpage_%subpage%_pick
If ((f_number>= 1326) And (f_number<=1559))
{
f:= function_call%f_number%
paddy_slider2 := SubStr(f,4)
nocturn_count_for_paddy_slider_%paddy_slider2%++
count2 := nocturn_count_for_paddy_slider_%paddy_slider2%
mapped_to_nocturn_cc_%paddy_slider2%_%count2%  := A_Index
mapped_to_nocturn_channel_%paddy_slider2%_%count2%  := subpage
slider_%paddy_slider2%_is_mapped_to_midi:=1
}
}
}
Return
}
