Activate_LR()
{
global
If (verbose_debug)
_paddyddmessage("Starting search for LR", A_ThisFunc)
SetTitleMatchMode,2
WinActivate, ahk_class AgWinMainFrame
start := A_TickCount
If (max_trial<2000)
max_trial := 2000
Loop
{
IfWinActive, Lightroom ahk_class AgWinMainFrame
{
If (debugmode)
_PaddyDDmessage("Found LR", A_ThisFunc)
WinGet hWnd, ID, Lightroom ahk_class AgWinMainFrame
LRMenuHandle := DllCall("GetMenu", "UInt", hWnd)
Break
}
If (start< (A_TickCount - max_trial))
{
If (DEBUGMODE)
_PaddyDDMessage("Did not find LR",A_ThisFunc)
MsgBox, 4148,%my_name%: A problem occured, Could Not switch to Lightroom - Continue to try?
IfMsgBox, No
Return False
Else
{
If (VERBOSE_DEBUG)
_PaddyDDMessage("Trying again",A_ThisFunc)
start := A_TickCount
}
}
}
Return True
}
read_presets()
{
global
WinActivate, ahk_class AgWinMainFrame
LRactive :=1
If save_switch_module
{
Shifting_module:=1
save_switch_module:=0
}
Else
Shifting_module:=0
If (VERBOSE_DEBUG)
_PaddyDDMessage("Trying to get control handles",A_ThisFunc)
poll_box_handle_function()
If (have_library_handles)
{
If ( DEBUGMODE)
_PaddyDDMessage("Handles are valid already, starting preset read",A_ThisFunc)
}
Else If (in_lr_library())
poll_box_handle_function()
Else
{
If (DEBUGMODE)
_PaddyDDMessage("No Handles yet - switching to Library",A_ThisFunc)
shifting_module := 1
force_library_module_switch()
}
WinActivate, ahk_class AgWinMainFrame
If (have_library_handles)
{
If (DEBUGMODE)
_PaddyDDMessage("Valid Handles",A_ThisFunc)
If (VERBOSE_DEBUG)
_PaddyDDMessage("preset box handle:" . preset_box_handle,A_ThisFunc)
ControlGet preset_list1, List,,, ahk_id %preset_box_handle%
If (DEBUGMODE)
_PaddyDDMessage("Pulled Development Preset List",A_ThisFunc)
If (menu_DEBUG)
_PaddyDDMessage("----- Development Preset list -----",A_ThisFunc)
max_presets := 0
Loop, Parse, preset_list1, `n
{
max_presets++
preset%max_presets% := A_LoopField
If (menu_DEBUG)
_PaddyDDMessage("||||||||||||| " . A_LoopField,A_ThisFunc)
If (preset%max_presets% = CustomPreset)
max_presets--
}
If (menu_DEBUG)
_PaddyDDMessage("----- End Development Preset list -----",A_ThisFunc)
If (VERBOSE_DEBUG)
_PaddyDDMessage("parsed development preset list - " . max_presets . "entries",A_ThisFunc)
ControlGet preset_list2, List,,, ahk_id %filter_box_handle%
If (DEBUGMODE)
_PaddyDDMessage("Pulled Filter Preset List",A_ThisFunc)
If (menu_DEBUG)
_PaddyDDMessage("----- Filter Preset list -----",A_ThisFunc)
max_filter_presets := 0
Loop, Parse, preset_list2, `n
{
if(InStr(A_LoopField,"Save Current Settings") +InStr(A_LoopField,"Delete Preset") + InStr(A_LoopField,"Rename Preset") +InStr(A_LoopField,"---------") = 0)
{
max_filter_presets++
filter_preset%max_filter_presets% := A_LoopField
If (menu_DEBUG)
_PaddyDDMessage("||||||||||||| " . A_LoopField,A_ThisFunc)
}
}
If (menu_DEBUG)
_PaddyDDMessage("----- End Filter Preset list -----",A_ThisFunc)
If (VERBOSE_DEBUG)
_PaddyDDMessage("parsed filter preset list - " . max_filter_presets . "entries",A_ThisFunc)
}
Else
{
If (DEBUGMODE)
_PaddyDDMessage("No valid handles->no Development or Filter Presets available",A_ThisFunc)
max_presets := 0
max_filter_presets :=0
}
If (DEBUGMODE)
_PaddyDDMessage("getting external edit presets",A_ThisFunc)
If in_lr_develop()
main_menu := edit_in_main_develop
Else If in_lr_library()
main_menu := edit_in_main_library
submenu_handle:=GetsubMenuHandle(main_menu-1, Edit_in_menu_text)
If ((in_lr_develop()) or (in_lr_library()))
{
Loop
{
i:=A_Index+2
l := DllCall("GetMenuString"
, "UInt", submenu_handle
, "UInt", i
, "Str", LRMenuString
, "Int", 50
, "UInt", 0x0400)
If (lrmenustring = open_as_smart_object_menu_text)
{
external_application_count := A_Index - 2
Break
}
external_application%A_Index% := LRmenustring
If (menu_DEBUG)
_paddyddmessage("Found external application: " . lrmenustring,A_ThisFunc)
If (A_Index>50)
Break
}
}
If (VERBOSE_DEBUG)
_PaddyDDMessage("Found " . external_application_count . " External application presets" ,A_ThisFunc)
If (DEBUGMODE)
_PaddyDDMessage("getting export preset list",A_ThisFunc)
export_preset_menu_handle := GetExportPresetMenuHandle()
export_preset_entry_count:=DllCall("GetMenuItemCount",UInt,export_preset_menu_handle)
If (VERBOSE_DEBUG)
_paddyddmessage("Number of export preset entries:  " . export_preset_entry_count ,A_ThisFunc)
If (menu_DEBUG)
_PaddyDDMessage("----- Export Preset list -----",A_ThisFunc)
Loop, %export_preset_entry_count%
{
i:=A_Index-1
l := DllCall("GetMenuString"
, "UInt", export_preset_menu_handle
, "UInt", i
, "Str", LRMenuString
, "Int", 50
, "UInt", 0x0400)
export_preset%A_Index% := LRmenustring
If (menu_DEBUG)
_PaddyDDMessage("||||||||||||| " . lrmenustring,A_ThisFunc)
}
If (menu_DEBUG)
_PaddyDDMessage("----- End Export Preset list -----",A_ThisFunc)
WinActivate, ahk_class AgWinMainFrame
If (shifting_module = 1)
Send ^!{UP}
Return
}
force_library_module_switch(talk = 1)
{
global
delayed_library_switch:=0
try_library_module_switch_again:
WinActivate, ahk_class AgWinMainFrame
Send {Esc}
Send ^!1
start := A_TickCount
If (talk)
paddymessage2(m_temp_switch, max_trial)
If (DEBUGMODE)
_PaddyDDMessage("Forcing Switch to Library",A_ThisFunc)
While !in_lr_library()
{
Sleep, 100
If (start < (A_TickCount - max_trial))
{
If (DEBUGMODE)
_PaddyDDMessage("Could not switch to Library Module",A_ThisFunc)
SetTimer, library_module_alert, 300
MsgBox, 4148, %my_name%: %G_a_problem_occured%, %G_library_error%
IfMsgBox, Yes
{
If (VERBOSE_DEBUG)
_PaddyDDMessage("User is trying again",A_ThisFunc)
SetTimer, library_module_alert, Off
Goto try_library_module_switch_again
}
IfMsgBox, No
{
If (delayed_library_switch)
{
If (Verbose debug)
_PaddyDDMessage("Timer found Library switch",A_ThisFunc)
}
Else
{
If (VERBOSE_DEBUG)
_PaddyDDMessage("User cancelled Library Switch",A_ThisFunc)
shifting_module = 0
}
Break
}
}
}
SetTimer, library_module_alert, Off
Gosub Killsplash
WinActivate, ahk_class AgWinMainFrame
poll_box_handle_function()
Return
}
library_module_alert:
{
If in_lr_library_fast()
{
_paddyddmessage("Switched to Library Module", A_ThisFunc)
delayed_library_switch := 1
WinActivate, %my_name%: %G_a_problem_occured%
ControlClick, button2, %my_name%: %G_a_problem_occured%
}
Else
{
If (VERBOSE_DEBUG)
_paddyddmessage("Timer: not in Library yet!", A_ThisFunc)
}
Return
}
