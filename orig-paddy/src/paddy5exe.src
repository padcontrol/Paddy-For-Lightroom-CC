get_me_function_name(keyname)
{
global
function_number:= %keyname%_pick
function_source:= %keyname%_keyset
__paddyddmessage(keyname .  "   keyset: " . function_source,A_ThisFunc)
If (function_number  = 2) or (function_number  = 307) or (function_number  = 1279) or (function_number  = 1283) or (function_number  = 1281) or (function_number  = 1280) or (function_number  = 2357) or (function_number  = 2619) or (function_number  = 8)
r:= %keyname%_preset_pick
Else If (function_number  = 306)
{
r1:= %keyname%_preset_pick
r:= macro_name%r1%
}
Else If (function_number  = 9)
{
r1:= %keyname%_preset_pick
r:= key_set_name_%r1%
}
Else If (function_number  = 1271)
{
r1:= %keyname%_preset_pick
r:= brush_%r1%_name
}
Else
{
r:= function_Desc%function_number%
}
If (!r)
{
r:="Not supported"
}
If ((function_number=8) And reverse_order)
{
r:=create_reverse_keyword2(r)
StringReplace,r,r,%A_Tab%,->,All
}
If function_source
{
If (((function_source<>current_key_set) And (function_number>1)) And (!InStr(keyname,"chord") And !InStr(keyname,"midi") And !InStr(keyname,"pvc_") And !InStr(keyname,"note_")))
r.=" (" . function_source . ")"
}
Return r
}
get_me_a_function(pk, ck)
{
global
plain_key := pk
current_key := ck
current_function:= %current_key%_pick
current_preset:= %current_key%_preset_pick
_paddyddmessage("Current key : " . current_key . ", current function : " . current_function . ", current preset : " . current_preset  , A_ThisFunc)
found := 0
If (current_function = 306)
{
g:=macro_group
o:= current_preset
found :=1
}
If (current_function = 3)
{
g:=Midi_message_group
o:= current_preset
found :=1
}
Else If (current_function = 9)
{
g:=keyset_group
o:= current_preset
found :=1
}
Else if (current_function = 307)
{
g:=camera_profile_group
search_for := current_preset
Loop, 30
{
If (search_for = camera_profile_%A_Index%)
{
o:= A_Index
found :=1
Break
}
}
}
Else if (current_function = 2)
{
g:=preset_group
search_for := current_preset
_PaddyDDMessage("assigned: >" . current_preset  . "<", A_ThisFunc)
Loop, %max_presets%
{
If (VERBOSE_DEBUG)
_PaddyDDMessage("loaded >" . preset%A_Index% . "<", A_ThisFunc)
If (search_for = preset%A_Index%)
{
o:= A_Index
found :=1
Break
}
}
}
Else if (current_function = 2357)
{
g:=export_preset_group
search_for := current_preset
_PaddyDDMessage("assigned: >" . current_preset  . "<", A_ThisFunc)
Loop, %export_preset_entry_count%
{
If (VERBOSE_DEBUG)
_PaddyDDMessage("loaded >" . preset%A_Index% . "<", A_ThisFunc)
If (search_for = export_preset%A_Index%)
{
o:= A_Index
found :=1
Break
}
}
}
Else if (current_function = 2619)
{
g:=external_application_group
search_for := current_preset
_if (VERBOSE_DEBUG)
_PaddyDDMessage("assigned >" . current_preset  . "<", A_ThisFunc)
Loop, %external_application_count%
{
