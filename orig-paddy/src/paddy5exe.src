start_vr:
{
vr_start()
Return
}
vr_start()
{
global
If !FileExist(voice_command_file)
update_paddy_voice_commands()
voicecommands:=""
Loop, Read, %voice_command_file%
{
StringSplit, parameter,A_LoopReadLine,=
%parameter1%:= parameter2
If InStr(parameter1,"_command")
{
StringTrimRight,parameter1,parameter1,8
StringTrimLeft,parameter1,parameter1,4
voicecommands .=  "," . parameter1
}
}
StringTrimLeft,voicecommands,voicecommands,1
Process, Exist,sapisvr.exe
startAudioInput := ErrorLevel ? False : True
COM_Init()
plistener:= COM_CreateObject("SAPI.SpSharedRecognizer")
If startAudioInput
COM_Invoke(plistener, "AudioInput", paudioin ? "+" . paudioin : "+0")
pcontext := COM_Invoke(plistener, "CreateRecoContext")
pgrammar := COM_Invoke(pcontext , "CreateGrammar")
COM_Invoke(pgrammar, "DictationSetState", 0)
prules := COM_Invoke(pgrammar, "Rules")
prulec := COM_Invoke(prules, "Add", "wordsRule", 0x1|0x20)
COM_Invoke(prulec, "Clear")
pstate := COM_Invoke(prulec, "InitialState")
Loop, Parse, voicecommands, `,
{
k:="pvc_" . A_LoopField . "_Command"
k:=%k%
COM_Invoke(pstate, "AddWordTransition", "+" . 0, k)
}
COM_Invoke(prules, "Commit")
COM_Invoke(pgrammar, "CmdSetRuleState", "wordsRule", 1)
pevent := COM_ConnectObject(pcontext, "On")
Return
}
OnRecognition(prms, this)
{
global
presult := COM_DispGetParam(prms, 3, 9)
pphrase := COM_Invoke(presult, "PhraseInfo")
sText   := COM_Invoke(pphrase, "GetText")
COM_Release(pphrase)
If !WinActive("AHK_CLASS AgWinMainFrame")
Return
If (DEBUGMODE)
_PaddyDDMessage("Voice command received:" . stext,A_ThisFunc)
new_command := "pvc_" . convert_to_variable(stext)
key99996 := new_command
execute(99996)
Return
}
