recording_key_pressed:
{
this_hk := a_thishotkey
Stringreplace, this_hk, This_hk,%a_space%UP
remove_mods(this_hk)
if (DEBUGMODE)
_PaddyDDMessage("Hotkey in recorder: " . this_hk,a_thislabel)
found:=0
stringupper,this_hk,this_hk
n := max_keys/9
loop, %n%
{
hk_n:=(a_index-1)*9 +1
if (get_hotkey(hk_n) = this_hk)
{
found :=1
break
}
}
pressed_key_modifier:=""
If (getkeystate("LWIN") OR getkeystate("RWIN"))
pressed_key_Modifier .= "#"
else
{
If getkeystate("CTRL")
pressed_key_Modifier .= "^"
If getkeystate("ALT")
pressed_key_Modifier .= "!"
If getkeystate("SHIFT")
pressed_key_Modifier .= "+"
}
if (pressed_key_modifier = "#")
hk_n +=1
if (pressed_key_modifier = "^")
hk_n +=2
if (pressed_key_modifier = "!")
hk_n +=3
if (pressed_key_modifier = "+")
hk_n +=4
if (pressed_key_modifier = "^!")
hk_n +=5
if (pressed_key_modifier = "^+")
hk_n +=6
if (pressed_key_modifier = "!+")
hk_n +=7
if (pressed_key_modifier = "^!+")
hk_n +=8
if (!found)
{
kn := pressed_key_modifier . this_hk
recordkey(kn)
send %kn%
return
}
kn := key%hk_n%
if (%kn%_pick > 1)
execute(hk_n)
Else
{
recordkey(skey%hk_n%)
sk:= skey%hk_n%
send %sk%
}
return
}
