Find_setting_tags(filename,ByRef has_keyboard,ByRef has_midi_controller,ByRef has_midi_keyboard,ByRef has_sliderdeltas,ByRef has_brushpresets,ByRef has_macros,ByRef has_cameraprofiles)
{
has_keyboard:=has_midi_controller:=has_midi_keyboard:=has_sliderdeltas:=has_brushpresets:=has_macros:=has_cameraprofiles:=0
in_values :=0
Loop, Read, %filename%
{
If A_LoopReadLine=
Continue
If (A_LoopReadLine = "[slider_settings]")
has_sliderdeltas:=1
If (A_LoopReadLine = "[Camera_Profiles]")
has_cameraprofiles:=1
If (A_LoopReadLine = "[Brush_presets]")
has_brushpresets:=1
If (A_LoopReadLine = "[Macros]")
has_macros :=1
If (A_LoopReadLine = "[values]")
{
in_values := 1
Continue
}
Else if (InStr(A_LoopReadLine,"[") And InStr(A_LoopReadLine,"]"))
{
in_values :=0
Continue
}
If in_values
{
If ((InStr(A_LoopReadLine, "midi")))
has_midi_controller:=1
Else if ((InStr(A_LoopReadLine, "note")) or (InStr(A_LoopReadLine, "chord")))
has_midi_keyboard:=1
Else
has_keyboard:=1
}
}
Return
}
