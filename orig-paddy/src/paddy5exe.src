{
settimer,watchxbox,-1000
return
}
next_time:=-300
GetKeyState, Trigger, JoyZ
GetKeyState, LStickLR, JoyX
GetKeyState, LStickUD, JoyY
GetKeyState, RStickLR, JoyU
GetKeyState, RStickUD, JoyR
GetKeyState, JoyV, JoyV
GetKeyState, DPAD, JoyPOV
If Trigger
{
If Trigger < 30
xbox_execute("XBox_RTrigger")
Else If Trigger > 70
xbox_execute("XBox_LTrigger")
If LStickLR > 70
xbox_execute("XBox_LStick_Right",LStickLR-50)
Else if LStickLR < 30
xbox_execute("XBox_LStick_Left",50-LStickLR)
If LStickUD > 70
xbox_execute("XBox_LStick_Down",LStickUD-50)
Else if LStickUD < 30
xbox_execute("XBox_Lstick_Up",50-LStickUD)
If RStickLR > 70
xbox_execute("XBox_RStick_Right",RStickLR-50)
Else if RStickLR < 30
xbox_execute("XBox_RStick_Left",50-RStickLR)
If RStickUD > 70
xbox_execute("XBox_RStick_Down",RStickUD-50)
Else if RStickUD < 30
xbox_execute("XBox_RStick_Up",50-RStickUD)
If !((DPAD)=-1)
{
If (DPAD=0)
xbox_execute("XBox_DPAD_up")
Else if (DPAD=9000)
xbox_execute("XBox_DPAD_right")
Else if (DPAD=18000)
xbox_execute("XBox_DPAD_down")
Else if (DPAD=27000)
xbox_execute("XBox_DPAD_left")
Else if (DPAD=4500)
{
xbox_execute("XBox_DPAD_up")
xbox_execute("XBox_DPAD_right")
}
Else if (DPAD=13500)
{
xbox_execute("XBox_DPAD_down")
xbox_execute("XBox_DPAD_right")
}
Else if (DPAD=22500)
{
xbox_execute("XBox_DPAD_down")
xbox_execute("XBox_DPAD_left")
}
Else if (DPAD=31500)
{
xbox_execute("XBox_DPAD_up")
xbox_execute("XBox_DPAD_left")
}
}
}
SetTimer, WatchXBox,%next_time%
Return
}
xbox_execute(Xbox_variable,amount=50)
{
global
critical
If (in_midi_menu=99)
{
StringTrimLeft,Xbox_control,Xbox_variable,4
Xbox_control_handle:="handle_xbox_control" . xbox_control
clicker:=%Xbox_control_handle%
ControlClick,,ahk_id %clicker%
next_time:=-500
Return
}
key99997 := xbox_variable . "_page_" . current_paddy_xbox_page
xbox_proportional_slider_mode_left:=1
xbox_proportional_slider_mode_right:=1
If xbox_proportional_slider_mode_left
{
If InStr(xbox_variable,"stick")
{
a:=%key99997%_pick
a:=function_call%a%
p:=SubStr(a,1,3)
If InStr("mov,SSU,SSD,MSU,MSD,LSU,LSD",p)
{
if (instr(p,"d") or instr(a,"down"))
d:=2
else
d:=1
if instr(a,"move_selected")
q:=user_selected_slider
else
q:=substr(a,4,3)
if (amount=50)
{
Move_slider(q, 6, d,3)
next_time:=-50
}
Else if (amount>35)
{
Move_slider(q, 6, d,2)
next_time:=-100
}
Else if (amount>20)
{
Move_slider(q, 6, d,1)
}
return
}
}
}
execute(99997)
Return
}
