IniRead, toggle_button_%A_Index%_y,  %paddy_settingsdir%\paddy_keyboardfile.kbd, Toggles, Toggle_%A_Index%_y
}
number_of_keys := horizontal_count * vertical_count
layer_offset := horizontal_count * vertical_count - unassigned_key_count - toggle_key_count
skip_key_count := unassigned_key_count + toggle_key_count
IniWrite, %selection_name%, %target_fn%,NAME,selection_name
IniWrite, %menu_name%, %target_fn%,NAME,menu_name
IniWrite, %gui_name%, %target_fn%,NAME,gui_name
IniWrite, %key_name%, %target_fn%,NAME,key_name
