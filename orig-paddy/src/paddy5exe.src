%parameter1%_pick := parameter2
Write_to_key_set(parameter1)
advance_progress(1)
}
}
Else If (Status = 2)
{
If (!(InStr(parameter1, "midi")) and !(InStr(parameter1, "note")) and !(InStr(parameter1, "chord")))
{
If (parameter2 = "ERROR") or (!parameter2)
parameter2 := " "
if ((%parameter1%_pick>1) or import_overwrites)
%parameter1%_preset_pick := parameter2
if(SubStr(%parameter1%_preset_pick ,0) = " ")
StringTrimRight, %parameter1%_preset_pick,%parameter1%_preset_pick,1
Write_to_key_set(parameter1)
advance_progress(1)
}
}
}
Return
}
