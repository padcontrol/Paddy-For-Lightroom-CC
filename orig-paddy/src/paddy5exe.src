clipping_is_good(ByRef x, ByRef y)
{
SendMessage, 0x432,0,0,, ahk_class AgWinMainFrame
Sleep, 500
PixelGetColor, triangle_color ,X, Y
Return % (triangle_color = "0x353535")
}
auto_black_white(k)
{
Auto_black(k)
Auto_white(k)
Return
}
alt_slider_mode_on(k)
{
global
alt_slider_mode:=True
paddymessage(m_alt_slider_mode_on)
Return
}
alt_slider_mode_off(k)
{
global
alt_slider_mode:=False
paddymessage(m_alt_slider_mode_off)
Return
}
toggle_alt_slider_mode(k)
{
global
If alt_slider_mode
alt_slider_mode_off(k)
Else
alt_slider_mode_on(k)
Return
}
