}
variable_preamble := TYP . "_2000_" . readpreset . "_" . parameter1 . "_" . parameter2
channel_variable := variable_preamble . "_channel"
cc_variable := variable_preamble . "_cc"
showvalue_variable := variable_preamble . "_showvalue"
resolution_variable := variable_preamble . "_resolution"
