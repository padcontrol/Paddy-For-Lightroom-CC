LongmessageListener(hInput, midiMsg, wMsg)
{
global
buffer_address := (*(midiMsg)) | (*(midiMsg+1) << 8 )| (*(midiMsg+2) << 16 )|(*(midiMsg+3) << 24)
message_from_bcr:= (buffer_address=bcr_buffer_address)
message_from_generic:= (buffer_address=generic_buffer_address)
message_from_bcf:= (buffer_address=bcf_buffer_address)
message_size := (*(midiMsg+8)) | (*(midiMsg+9) << 8 )| (*(midiMsg+10) << 16 )|(*(midiMsg+11) << 24)
If midi_debug_mode
_Mididebugmessage(wmsg . " Received Midi long message -  message size : " . message_size, A_ThisFunc)
buffer_address := (*(midiMsg)) | (*(midiMsg+1) << 8 )| (*(midiMsg+2) << 16 )|(*(midiMsg+3) << 24)
byte1 := *(buffer_address +1)
byte2 := *(buffer_address +2)
byte3 := *(buffer_address +3)
If  (byte1 = 0) And (byte2 = 0x20) And (byte3 = 0x32)
{
If midi_debug_mode
_MidiDebugMessage("  -->it's a Behringer sysex",A_ThisFunc)
byte6 := *(buffer_address +6)
byte7 := *(buffer_address +7)
If (message_size>23) And (byte6 = 0x21) And (byte7 = 0)
{
byte8 := *(buffer_address +8)
preset_name := ""
Loop, 24
{
read_byte := *(buffer_address +8+ A_Index)
preset_name .= Chr(read_byte)
}
If midi_debug_mode
_MidiDebugMessage("   --->Beringer reported preset#" . byte8 . " as name: " . preset_name,A_ThisFunc)
If (byte8 :=127)
Paddymessage(M_Switched_to . " " .  preset_name)
Else
Paddymessage(M_Behringer_Preset . byte8 . " " M_is_named . preset_name)
}
Else
{
If midi_debug_mode
_MidiDebugMessage("   --->Unsupported Behringer SysEx",A_ThisFunc)
}
}
If message_from_generic
add_midi_buffer(hinput, &generic_midi_header)
If message_from_bcr
add_midi_buffer(hinput, &bcr_midi_header)
If message_from_bcf
add_midi_buffer(hinput, &bcf_midi_header)
Return
}
midioutloop:
{
