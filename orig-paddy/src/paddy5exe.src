fast_read()
{
global
Status :=0
chordlist := voicecommands :=""
Loop, Read, %definitionfile%
{
If (substr(A_LoopReadLine,1,1)="[")
{
If (A_LoopReadLine = "[values]")
{
Status =1
If (verbose_DEBUG)
_PaddyDDMessage("found values", A_ThisFunc)
}
Else if (A_LoopReadLine = "[presets]")
{
Status =2
If (verbose_DEBUG)
_PaddyDDMessage("found presets", A_ThisFunc)
}
Else if (A_LoopReadLine = "[user_defined_keyboard]")
{
Status =3
If (verbose_DEBUG)
_PaddyDDMessage("found user keyboard", A_ThisFunc)
}
Else if (A_LoopReadLine = "[slider_settings]")
{
Status =4
If (verbose_DEBUG)
_PaddyDDMessage("found slider settings",A_ThisFunc)
}
Else if (A_LoopReadLine = "[Camera_Profiles]")
{
Status =5
If (verbose_DEBUG)
_PaddyDDMessage("found camera profiles",A_ThisFunc)
}
Else if (A_LoopReadLine = "[Brush_presets]")
{
Status =6
If (verbose_DEBUG)
_PaddyDDMessage("found brush presets",A_ThisFunc)
}
Else if (A_LoopReadLine = "[Macro_Names]")
{
Status =7
If (verbose_DEBUG)
_PaddyDDMessage("found Macro names",A_ThisFunc)
}
Else if (A_LoopReadLine = "[Macros]", A_ThisFunc)
{
Status =8
If (verbose_DEBUG)
_PaddyDDMessage("found Macros",A_ThisFunc)
}
Else if (A_LoopReadLine = "[Midi_messages]", A_ThisFunc)
{
Status =9
If (verbose_DEBUG)
_PaddyDDMessage("found Midi Messages",A_ThisFunc)
}
Else
{
If (verbose_DEBUG)
_PaddyDDMessage("found " . A_LoopReadLine . " ->unused in fast_read",A_ThisFunc)
Status = 0
}
Continue
}
StringSplit, parameter,A_LoopReadLine,=
If (Status = 1)
{
If (parameter2 = "ERROR") or (parameter2=" ") or (!parameter2)
parameter2 := 1
%parameter1%_pick := rtrim(parameter2)
%parameter1%_keyset:=1
if instr(parameter1,"chord")
chordlist .=  "," . parameter1
if instr(parameter1,"pvc_")
voicecommands .=  "," . parameter1
}
Else If (Status = 2)
{
If (parameter2 = "ERROR") or (!parameter2)
parameter2 := " "
%parameter1%_preset_pick := rtrim(parameter2)
}
Else If (Status = 3)
{
If (parameter2 = "ERROR") or (parameter2=" ") or (!parameter2)
parameter2 := %parameter1%
%parameter1% := parameter2
}
Else If (Status = 4)
{
If (parameter2 = "ERROR") or (!parameter2)  or (parameter2=" ")
parameter2 := %parameter1%
%parameter1% := parameter2
}
Else If (Status = 5)
{
If (parameter2 = "ERROR") or (!parameter2)
parameter2 := " "
%parameter1% := rtrim(parameter2)
}
Else If (Status = 6)
{
If (parameter2 = "ERROR") or (parameter2=" ") or (!parameter2)
{
If InStr(parameter1, "automask_ignore")
parameter2 := "0"
}
If (menu_DEBUG)
_PaddyDDMessage(parameter1 . " = " . parameter2 . "<", A_ThisFunc)
%parameter1% := parameter2
}
Else If (Status = 7)
{
StringTrimLeft, parameter1, parameter1, 6
If (parameter2 = "ERROR") or (!parameter2)
parameter2 := ""
If (menu_DEBUG)
_PaddyDDMessage("macro number: >" . parameter1 . "<  name: " . parameter2, A_ThisFunc)
macro_name%parameter1% := rtrim(parameter2)
}
Else If (Status = 8)
{
StringTrimLeft, parameter1, parameter1, 6
If (parameter2 = "ERROR") or (!parameter2)
parameter2 := ""
If (menu_DEBUG)
_PaddyDDMessage("macro number: >" . parameter1 . "<  keys to send: " . parameter2, A_ThisFunc)
macro_%parameter1% := parameter2
}
Else If (Status = 9)
{
StringTrimLeft, parameter1, parameter1, 13
If (parameter2 = "ERROR") or (!parameter2)
parameter2 := ""
If (menu_DEBUG)
_PaddyDDMessage("midi message number: >" . parameter1 . "<  message: " . parameter2, A_ThisFunc)
midi_message_%parameter1% := parameter2
}
}
stringtrimleft,chordlist,chordlist,1
stringtrimleft,voicecommands,voicecommands,1
Return
}
