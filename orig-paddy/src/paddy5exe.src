OSC_string_receiver(wParam,lparam)
{
Critical
global XkeysObject, midi_run, definitionfile,version_check_return_label,total_count,vtest,
StringAddress := NumGet(lParam + 2*A_PtrSize)
CopyOfData := StrGet(StringAddress)
If (wparam = 1)
OutputDebug, Received the following OSC data: %CopyOfData%
If (wparam = 2)
{
StringSplit,ipaddress,copyofdata,:
OutputDebug, IPAD reports as IP address: >%ipaddress1%<
Loop, 2
{
er:=COM_Invoke(XkeysObject, "set_osc_destination_ip_address",ipaddress1,9000)
Sleep,200
midi_run:=0
Gosub, midioutloop
}
IniWrite, %ipaddress1%,  %Definitionfile%, OSC, ipaddress
show_connect_menu(ipaddress1)
Return
}
If (wparam = 4)
OutputDebug, received string: %copyofdata%
If (wparam = 5)
{
%version_check_return_label%(copyofdata)
}
If (wparam = 6)
{
%version_check_return_label%(copyofdata)
}
If (wparam = 7)
{
OutputDebug, Total Count: %copyofdata%
Iniwrite, 1, %a_scriptdir%\debug_setting.ini, Install, %total_count%
}
If (wparam = 8)
{
Iniwrite, 1, %a_scriptdir%\debug_setting.ini, Install, %vtest%
OutputDebug, version Count: %copyofdata%
}
If (wparam = 9)
{
text:=copyofdata
If InStr(Text,"<nothing>")
Return
StringTrimLeft,Text,Text,1
nr:=""
Loop
{
If (SubStr(Text,1,1)=">")
Break
nr.=SubStr(Text,1,1)
StringTrimLeft,Text,Text ,1
}
StringTrimLeft,Text,Text,2
IniRead, last_message_number, %A_ScriptDir%\debug_setting.ini, Messages, last_message_number,0
If (last_message_number>=nr)
Return
MsgBox,4144,Important message,%Text%
IniWrite, %nr%, %A_ScriptDir%\debug_setting.ini, Messages, last_message_number
Return
}
Return
}
