send_a_sysex(guinumber,h_midiout)
{
global
if (use_any_midi_out =0)
{
msgbox, 4112,, %G_long_midi_error%
return
}
Gui, %guinumber%: Hide
SetWorkingDir %A_ScriptDir%
FileSelectFile, filename, 1, %paddy_templatedir%\Behringer\, Send .SYX file, Behringer setup file (*.syx)
If filename =
{
Gui, %guinumber%: Show
Return
}
SetTimer,midioutloop, Off
push2midi(filename, h_midiout,1)
SetBehringerpreset1("dummy")
If (use_any_midi_out >0)
SetTimer,midioutloop, -300
Gui, %guinumber%: Show
Return
}
Push2Midi(theFile, h_midiout, showprogress = 1)
{
global midi_sysex_send_delay
SplitPath , theFile, theFileDisplay
stop_long_message_listener()
BinRead(theFile,SysExMsgs)
ms := VarSetCapacity(SysExMsgs)
Address := &SysExMsgs
linecount := 0
curpos := 0
LineEnd =
BlockSize := 1
BlockCount :=0
if (showprogress = 1)
Progress, BM , Please do not interupt or start another application, Reading %theFileDisplay%
while (curpos < ms)
{
x := *(Address + curpos)
if (x = 247)
{
BlockCount++
if (BlockCount = BlockSize) or (curpos = ms)
{
linecount++
LineEnd%linecount% := curpos
BlockCount := 0
}
}
curpos++
if (showprogress=1)
{
dp := curpos/ms*100
Progress, %dp%
}
}
if (showprogress = 1)
{
progress, off
Progress, BM, Please do not interupt or start another application, Sending %theFileDisplay%
}
if (showprogress = 2)
{
progress, off
Progress, BM, Please do not interupt or start another application, Updating Behringer Preset
}
loop %LineCount%
{
if (A_Index = 1)
start := 0
else
{
start := end + 1
}
end := LineEnd%A_Index%
results := midiOutLongMsg(h_midiout,(Address+start),end - start)
if (results <> 0) 	{
progress, off
msgbox Core midi error: Error encountered sending a line %results%
break
}
if (showprogress)
{
dp := A_Index/LineCount*100
Progress, %dp%
}
sleep, %midi_sysex_send_delay%
}
if (showprogress)
progress, off
install_long_message_listener()
return
}
midiOutLongMsg(h_midiout, p_Address,  MsgSize)
{
MHDR_DONE := 0x1       /* done bit */
MHDR_PREPARED := 0x2       /* set if header prepared */
MHDR_INQUEUE := 0x4       /* reserved for driver */
MHDR_ISSTRM := 0x8
Global MIDIHDR
VarSetCapacity(MIDIHDR, 36, 0)
PokeInt(p_Address,&MIDIHDR)
PokeInt(MsgSize,    &MIDIHDR+4)
PokeInt(MsgSize,    &MIDIHDR+8)
result := DllCall("winmm.dll\midiOutPrepareHeader", UInt,h_midiout, UInt,&MIDIHDR, UInt,36, UInt)
If (result)  {
progress, off
MsgBox Core midi error Error %result% in midiOutPrepareHeader
Return -1
}
result := DllCall("winmm.dll\midiOutLongMsg",UInt,h_midiout,UInt,&MIDIHDR,UInt,36,UInt)
If (result or ErrorLevel)  {
er:= result . "," . ErrorLevel
progress, off
msgbox core midid error: OutputlongMsg error %er%
Return er
}
sleep 5
result := DllCall("winmm.dll\midiOutUnprepareHeader", UInt,h_midiout, UInt,&MIDIHDR, UInt,36, UInt)
If (result) {
progress, off
MsgBox core midi error: Error %result% in midiOutUnprepareHeader
Return -1
}
Return 0
}
