read_regional_messages(language)
{
global
IniRead, use_english,  %Definitionfile%, Settings, use_english, 0
if (use_english)
language := "English"
if (language<>"German")
language := "English"
fn := Paddy_resourcedir . "\" . language . ".plf4"
loop, read, %fn%
{
if substr(a_loopreadline,1,1)=";"
continue
stringsplit, parameter,a_loopreadline,=
if ((parameter0 = 0) or (parameter0 = 1))
continue
%parameter1% := parameter2
n:=2
Loop
{
if (parameter0=2)
break
n++
%parameter1% .= "=" . parameter%n%
parameter0--
}
stringreplace,%parameter1%,%parameter1%,``n,`n, All
}
return
}
