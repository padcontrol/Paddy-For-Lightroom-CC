get_real_key(virtual_key, Mod)
{
global
If (Virtual_key = "")
Return
If (DEBUGMODE)
_PaddyDDMessage("started - keyboard: " . keyboard . "  virtual key: " . virtual_key . "   modifier: " . Mod, A_ThisFunc)
If (keyboard = "User Defined")
{
If (Mod = "layer1")
real_key := %Virtual_key%
Else
{
virtual_key_number := SubStr(virtual_key, 8)
virtual_key_number := virtual_key_number + layer_offset
If (Mod = "layer3")
virtual_key_number := virtual_key_number + layer_offset
If (Mod = "layer4")
virtual_key_number := virtual_key_number + layer_offset
virtual_key := "userkey" . virtual_key_number
real_key := %Virtual_key%
}
}
Else if (keyboard = "Genovation 683" or keyboard = "Genovation 682")
{
virtual_key_number := SubStr(virtual_key, 8)
If (keyboard = "Genovation 683")
real_key := G683_%virtual_key_number%
Else
real_key := G682_%virtual_key_number%
If (Mod = "layer2")
real_key := "CTRL_" . real_key
}
Else if (keyboard = "XKeys (2 layers)" or keyboard = "XKeys (3 layers)")
{
virtual_key_number := SubStr(virtual_key, 8)
If (virtual_key_number < 51)
real_key := XKeys_%virtual_key_number%
Else if (keyboard = "XKeys (2 layers)")
real_key := XKeys_2_%virtual_key_number%
Else if (keyboard = "XKeys (3 layers)")
real_key := XKeys_3_%virtual_key_number%
If (Mod = "layer2" And real_key = XKeys_13)
real_key:= "CTRL_ALT_END"
Else if (Mod = "layer2")
real_key := "CTRL_" . real_key
If (Mod = "layer3")
real_key := XKeys_layer3_%virtual_key_number%
}
Else if (keyboard = "XKeys (New)")
{
virtual_key_number := SubStr(virtual_key, 8)
If (Mod = "layer1")
real_key := "XKeys_l_1_" . virtual_key_number
If (Mod = "layer2")
real_key := "XKeys_l_2_" . virtual_key_number
If (Mod = "layer3")
real_key := "XKeys_l_3_" . virtual_key_number
If (Mod = "layer4")
real_key := "XKeys_l_4_" . virtual_key_number
_PaddyDDMessage("Real key pick >" . %real_key%_pick . "<" ,A_ThisFunc)
If !(%real_key%_pick)
%real_key%_pick:=1
}
Else if ((keyboard = "keyboardpack") or (External_keyboard_Menu = "keyboardpack"))
{
virtual_key_number := SubStr(virtual_key, 8)
If (Mod = "layer1")
real_key =%keypack_key_name%_layer1_%virtual_key_number%
Else If (Mod = "layer2")
real_key =%keypack_key_name%_layer2_%virtual_key_number%
Else If (Mod = "layer3")
real_key =%keypack_key_name%_layer3_%virtual_key_number%
Else If (Mod = "layer4")
real_key =%keypack_key_name%_layer4_%virtual_key_number%
_PaddyDDMessage("Real key pick >" . %real_key%_pick . "<" ,A_ThisFunc)
If !(%real_key%_pick)
%real_key%_pick:=1
}
Else
MsgBox, 4112, %my_name% has a problem, Didn't find keyboard. This shouldn't happen. Please Report this bug
If (DEBUGMODE)
_PaddyDDMessage("done - returning:" . real_key, A_ThisFunc)
Return real_key
}
