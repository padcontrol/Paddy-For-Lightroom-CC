{
Status =1
If (verbose_DEBUG)
_PaddyDDMessage("found values", A_ThisFunc)
}
Else if (A_LoopReadLine = "[presets]")
{
Status =2
If (verbose_DEBUG)
_PaddyDDMessage("found presets", A_ThisFunc)
}
Else
Status = 0
Continue
}
StringSplit, parameter,A_LoopReadLine,=
If (Status = 1)
{
if (instr(parameter1,"chord") or instr(parameter1,"pvc_") or instr(parameter1,"note_"))
continue
if ( instr(parameter1,"midi") and (!(keyset_behavior_for_raw_midi and raw_midi_enable)))
continue
If (parameter2 = "ERROR") or (parameter2=" ") or (!parameter2)
parameter2 := 1
if (instr(parameter1,"xkeysnew") and (xkeys=0))
continue
if (!instr(parameter1,"xkeysnew") and (keyboard=0))
continue
