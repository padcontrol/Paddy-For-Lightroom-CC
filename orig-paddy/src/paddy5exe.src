Get_midi_for_function(this_function_number, this_function_preset, ByRef midi_channel, ByRef midi_cc, collect_garbage = 1)
{
global
midi_number := midi_for_function_%this_function_number%
If ((midi_number!=0) and (midi_number<>""))
{
midi_channel:= midi_number>>8
midi_cc := midi_number & 255
Return
}
cvc:=	is_already_mapped_to_midi(this_function_number, this_function_preset, midi_channel, midi_cc)
If !is_already_mapped_to_midi(this_function_number, this_function_preset, midi_channel, midi_cc)
{
If (find_next_free_midi_spot(midi_channel, midi_cc, collect_garbage))
{
midi_mapping_tracker_%midi_channel%_%midi_cc%_function := this_function_number
midi_mapping_tracker_%midi_channel%_%midi_cc%_preset := this_function_preset
}
Else
{
MsgBox, %G_midi_spot_error%
midi_channel:="n/a"
midi_cc:="n/a"
}
}
Return
}
Find_next_free_midi_spot(ByRef channel, ByRef cc, collect_garbage = 1)
{
global
if collect_garbage
Gui_garbage_collection()
Loop, 3
{
chan := 13 + A_Index
Loop, 127
{
If (midi_mapping_tracker_%chan%_%A_Index%_function = 0)
{
channel := chan
cc := A_Index
Return True
}
}
}
Return False
}
is_already_mapped_to_midi(this_function, this_preset, ByRef channel, ByRef cc)
{
global
Loop, 3
{
chan := 13 + A_Index
Loop, 127
{
If (midi_mapping_tracker_%chan%_%A_Index%_function = this_function) And (midi_mapping_tracker_%chan%_%A_Index%_preset = this_preset)
{
channel := chan
cc := A_Index
_paddyddmessage("Already mapped to channel " . chan . ", cc: " . cc, A_ThisFunc)
Return True
}
}
}
Return False
}
Gui_garbage_collection()
{
global
_paddyddmessage("started", A_ThisFunc)
Loop, 3
{
chan := 13 + A_Index
Loop, 127
{
If ((midi_mapping_tracker_%chan%_%A_Index%_function = 0) or !midi_mapping_tracker_%chan%_%A_Index%_function)
Continue
test_function := midi_mapping_tracker_%chan%_%A_Index%_function
test_preset := midi_mapping_tracker_%chan%_%A_Index%_preset
found :=False
Loop, 32
{
p:=A_Index
Loop, 8
{
If ( bcf_2000_%p%_fader_%A_Index%_pick = test_function) And ( bcf_2000_%p%_fader_%A_Index%_preset_pick = test_preset)
{
found := True
Break
}
}
If found
Break
Loop, 56
{
If ( bcf_2000_%p%_button_%A_Index%_pick = test_function) And ( bcf_2000_%p%_button_%A_Index%_preset_pick = test_preset)
{
found := True
Break
}
If ( bcf_2000_%p%_encoder_%A_Index%_pick = test_function) And ( bcf_2000_%p%_encoder_%A_Index%_preset_pick = test_preset)
{
found := True
Break
}
If ( bcr_2000_%p%_button_%A_Index%_pick = test_function) And ( bcr_2000_%p%_button_%A_Index%_preset_pick = test_preset)
{
found := True
Break
}
If ( bcr_2000_%p%_encoder_%A_Index%_pick = test_function) And ( bcr_2000_%p%_encoder_%A_Index%_preset_pick = test_preset)
{
found := True
Break
}
}
If found
Break
}
If !found
{
midi_mapping_tracker_%chan%_%A_Index%_function:=0
midi_mapping_tracker_%chan%_%A_Index%_preset:=""
_paddyddMessage("freeing up channel " . chan . ", cc: " . A_Index, A_ThisFunc)
}
}
}
Return
}
