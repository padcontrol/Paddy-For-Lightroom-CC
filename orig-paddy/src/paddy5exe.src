{
y := (A_Index - 1)
Loop, %horizontal_count%
{
x := (A_Index - 1)
skip:=0
Loop, %unassigned_key_count%
{
If ((skip_%A_Index%_x -1= x) And (skip_%A_Index%_y -1= y))
skip:=1
}
Loop, %toggle_key_count%
{
If ((toggle_button_%A_Index%_x-1 = x) And (toggle_button_%A_Index%_y-1= y))
{
If (toggle_x="")
toggle_x:=x
Else
toggle_x.="," . x
If (toggle_y="")
toggle_y:=y
Else
toggle_y.="," . y
skip:=1
}
}
If skip
Continue
If (button_x="")
{
button_x:=x
button_y:=y
button_w:=1
button_h:=1
}
Else
{
button_x .= "," . x
button_y .= "," . y
button_w .= ",1"
button_h .= ",1"
}
}
}
