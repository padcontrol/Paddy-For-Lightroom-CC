define_super_sensitive_range()
{
global
If need_slider_handles
read_all_slider_handles()
Loop, 125
{
paddy_sl := A_Index + 2
If (midi_count_for_paddy_slider_%paddy_sl% > 0)
{
If check_slider_policy_no_message(paddy_sl, rp1)
Continue
original_slider:= paddy_sl
If get_slider_handle_from_paddy_virtual_slider_number(paddy_sl, edit_handle,0,1)
Continue
ControlGetText, cv, , ahk_id %edit_handle%
StringReplace, cv,cv, %a_comma%,%a_dot%
StringReplace, cv,cv, %A_Space%,, All
StringReplace, cv,cv, + ,,
StringReplace, cv, cv, ? ,-,
If (paddy_sl = 3)
{
theoretical_range := 128 * 2
}
Else
theoretical_range:= (slider_high_sensitive_%original_slider% - slider_low_sensitive_%original_slider%)//2
half_theoretical_range := theoretical_range//2
estimated_upper := cv + half_theoretical_range
estimated_lower := cv - half_theoretical_range
If (estimated_upper > slider_high_%original_slider%)
{
estimated_upper := slider_high_%original_slider%
estimated_lower := estimated_upper - theoretical_range
}
If (estimated_lower < slider_low_%original_slider%)
{
estimated_lower := slider_low_%original_slider%
estimated_upper := estimated_lower+ theoretical_range
}
super_sensitive_high_%original_slider% := estimated_upper
super_sensitive_low_%original_slider% := estimated_lower
}
}
Return
}
Midi_set_slider(paddy_slider_midi, midivalue,relative_mode = 0)
{
global
If ignore_short_input
{
if((A_TickCount - time_slider_moved_by_midi_out_%paddy_slider_midi% )<time_to_wait_if_slider_was_moved)
Return
}
If (!LRactive)
{
If midi_debug_mode
_MidiDebugMessage("       -->Not in LR",A_ThisFunc)
Return
}
Else if in_lr_develop_fast()
{
If need_slider_handles
read_all_slider_handles()
original_midi_slider:=paddy_slider_midi
r:= get_slider_handle_from_paddy_virtual_slider_number(paddy_slider_midi, edit_handle)
If r
{
If (use_solo_mode) And (r=1) And !(get_develop_sub_panel_status() & 8)
Send {Esc}^3
Return
}
n:= is_decimal_%paddy_slider_midi%
ControlGetText, current_value, , ahk_id %edit_handle%
If (relative_mode = 1)
{
StringReplace, current_value,current_value,`,,.,
StringReplace, current_value, current_value, %A_Space%,, All
StringReplace, current_value, current_value, + ,,
StringReplace, current_value, current_value, ? ,-,
delta := (midivalue&63)
If delta = 0
Return
If (original_midi_slider = 3)
{
midi_out_value := Temp_to_midi(current_value)
If ((midivalue)<64)
midivalue := midi_out_value  + delta
Else
midivalue := midi_out_value - delta
If (midivalue > 127)
midivalue := 127
If (midivalue < 0)
midivalue := 0
new_value := Round(midi_to_temp(midivalue),0)
}
Else
{
If !(is_decimal_%paddy_slider_midi%)
{
If (midi_value <64)
new_value:= current_value + delta
Else
new_value:= current_value - delta
}
Else
{
percentage:= Round(100*(current_value - slider_low_%Paddy_slider_midi%)/(Slider_range_normal_%paddy_slider_midi%),0)
If (midi_value <64)
new_percentage:= percentage + delta
Else
new_percentage:= percentage - delta
new_value:= slider_low_%paddy_slider_midi% + new_percentage/100*(Slider_range_normal_%paddy_slider_midi%)
}
}
If ((new_value) > slider_high_%paddy_slider_midi%)
new_value := slider_high_%paddy_slider_midi%
Else If ((new_value) < slider_low_%paddy_slider_midi%)
new_value := slider_low_%paddy_slider_midi%
}
Else
{
If ((original_midi_slider = 3) And !is_jpg())
new_value := Round(midi_to_temp(midivalue),0)
Else
{
If (super_sensitivity_mode = 1)
new_value := Round(super_sensitive_low_%original_midi_slider% + (midivalue/127) * (super_sensitive_high_%original_midi_slider% - super_sensitive_low_%original_midi_slider%), N )
Else If (high_sensitivity_mode = 1)
new_value := Round(slider_low_sensitive_%Paddy_slider_midi% + ((midivalue) * (slider_range_reverse_factor_sensitive_%Paddy_slider_midi%)), N )
Else
new_value := Round(slider_low_%Paddy_slider_midi% + ((midivalue) * (slider_range_reverse_factor_normal_%Paddy_slider_midi%)), N )
}
}
set_slider(Paddy_slider_midi, edit_handle, new_value, displayformidi)
midi_memory_%original_midi_slider% := new_value
If midi_debug_mode
_MidiDebugMessage("    -->moved slider " . 	slider_label_%Paddy_slider_midi% . " from " . current_value . " to " . new_value,A_ThisFunc)
If (original_midi_slider<>last_slidernumber)
{
memory_slider_%original_midi_slider% := current_value
have_memory_%original_midi_slider%:=1
}
last_slidernumber := original_midi_slider
}
Else
{
If (DEBUGMODE)
_paddyddMessage("    -->Not in Develop Module",A_ThisFunc)
}
Return
}
Temp_to_midi(temp)
{
global high_sensitivity_mode
If (high_sensitivity_mode = 1)
{
If (temp<=3000)
Return 0
If (temp<=7846)
{
a :=  (temp-3000)/46.6
Return a
}
If (temp<=10000)
{
a :=  (temp-7846)/93.65  +103
Return a
}
If (temp>10000)
Return 127
}
Else
{
If (temp<=7846)
{
a := (temp-2000)/82.34
Return a
}
If (temp<=10434)
{
a := (temp-7846)/172.53  + 71
Return a
}
If (temp<=18888)
{
a :=  (temp-10434)/384.27 + 86
Return a
}
a :=  (temp-18888)/1637.47 + 108
Return a
}
}
midi_to_temp(midi)
{
global high_sensitivity_mode, super_sensitivity_mode, super_sensitive_low_3, super_sensitive_high_3
If (super_sensitivity_mode = 1)
{
a := Round(super_sensitive_low_3 + (midi/127) * (super_sensitive_high_3 - super_sensitive_low_3), 0 )
Return a
}
Else If (high_sensitivity_mode = 1)
{
If (midi <=104)
{
a := 3000 + midi*46.6
Return a
}
a := 7846 + (midi-104)*93.65
Return a
}
Else
{
If (midi <=71)
{
a := 2000 + midi*82.34
Return a
}
If (midi <=86)
{
a := 7846 + (midi-71)*172.53
Return a
}
If (midi <=108)
{
a := 10434 + (midi-86)*384.27
Return a
}
a := 18888 + (midi-108)*1637.47
Return a
}
}
MidiInsList(ByRef NumPorts)
{
local List, MidiInCaps, PortName, result
VarSetCapacity(MidiInCaps, 50, 0)
VarSetCapacity(PortName, 32)
NumPorts := DllCall("winmm.dll\midiInGetNumDevs")
Loop %NumPorts%
{
result := DllCall("winmm.dll\midiInGetDevCapsA", UInt,A_Index-1, UInt,&MidiInCaps, UInt,50, UInt)
If (result OR ErrorLevel) {
List .= "|-Error-"
Continue
}
DllCall("RtlMoveMemory", Str,PortName, UInt,&MidiInCaps+8, UInt,32)
List .= "|" PortName
}
Return SubStr(List,2)
}
MidiInGetNumDevs()
{
