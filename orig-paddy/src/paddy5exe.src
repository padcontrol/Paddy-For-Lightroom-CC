First_run()
{
global definitionfile, paddy_settingsdir, paddy_resourcedir, paddy_inifile, include_user_external_keyboard, my_name, debugmode,Paddy_old_userdir,my_name,paddy_userdir
if (VERBOSE_DEBUG)
_PaddyDDMessage("First run started",a_thisfunc)
if fileexist(Paddy_old_userdir . "\Settings\default.psf")
{
CopyFilesAndFolders(Paddy_old_userdir . "\*.*", paddy_userdir, true)
filecopy, %paddy_settingsdir%\paddy.ini,%paddy_settingsdir%\%Paddy_inifile%
Loop,15
{
Slider := a_index+110
IniWrite, 1,  %Definitionfile%, Slider_Settings, slider_delta_1_%Slider%
IniWrite, 5,  %Definitionfile%, Slider_Settings, slider_delta_2_%Slider%
IniWrite, 10,  %Definitionfile%, Slider_Settings, slider_delta_3_%Slider%
}
IniWrite, 0.02,  %Definitionfile%, Slider_Settings, slider_delta_1_113
IniWrite, 0.1,  %Definitionfile%, Slider_Settings, slider_delta_2_113
IniWrite, 0.2,  %Definitionfile%, Slider_Settings, slider_delta_3_113
run, %my_name%.exe
exitapp
}
If !FileExist(paddy_settingsdir . "\" . Paddy_inifile)
{
if (DEBUGMODE)
_PaddyDDMessage("Creating Ini File",a_thisfunc)
IniWrite, %A_WorkingDir%,  %paddy_settingsdir%\%Paddy_inifile%, Settings, Directory
IniWrite, %version%,  %paddy_settingsdir%\%Paddy_inifile%, Settings, Version
IniWrite, Default.psf, %paddy_settingsdir%\%Paddy_inifile%, Settings, Settings_used
definitionfile := paddy_settingsdir . "\default.psf"
}
If !FileExist(definitionfile)
{
if (DEBUGMODE)
_PaddyDDMessage("Copying Original Settings File",a_thisfunc)
s := Paddy_resourcedir . "\" . my_name . "settings.Default"
FileCopy, %s%, %definitionfile%
}
if (include_user_external_keyboard)
{
If !FileExist(paddy_settingsdir . "\paddy_keyboardfile.kbd")
{
FileCopy, %Paddy_resourcedir%\paddy_keyboardfile.kbd, %paddy_settingsdir%\paddy_keyboardfile.kbd
}
