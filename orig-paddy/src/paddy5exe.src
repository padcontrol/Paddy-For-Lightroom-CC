create_sysex_for_Preset(BCTYPE, p)
{
global
preset_output:=""
midi_channel:=midi_cc:=0
If (BCTYPE = "BCF")
{
encoder_count := 32
button_count:=56
}
Else
{
Encoder_count:=56
button_count:=56
}
preset_output .= BCL_Preset_start_Section(BCTYPE, p, %BCTYPE%_Preset_%p%_name)
Loop, %encoder_count%
{
a :=%BCTYPE%_2000_%p%_encoder_%A_Index%_pick
If (a!=1)
{
Get_midi_for_function(a, %BCTYPE%_2000_%p%_encoder_%A_Index%_preset_pick,  midi_channel,  midi_cc)
preset_output .= BCL_encoder_section(BCTYPE, A_Index, function_desc%a%, midi_channel, midi_cc, %BCTYPE%_2000_%p%_encoder_%A_Index%_showvalue,%BCTYPE%_2000_%p%_encoder_%A_Index%_resolution, %BCTYPE%_2000_%p%_encoder_%A_Index%_led)
}
}
Loop, %Button_count%
{
a :=%BCTYPE%_2000_%p%_button_%A_Index%_pick
If (a!=1)
{
Get_midi_for_function(a, %BCTYPE%_2000_%p%_button_%A_Index%_preset_pick,  midi_channel,  midi_cc)
preset_output .= BCL_button_section(BCTYPE, A_Index, function_desc%a%, midi_channel, midi_cc, %BCTYPE%_2000_%p%_button_%A_Index%_showvalue, %BCTYPE%_2000_%p%_button_%A_Index%_toggle)
}
}
If (BCTYPE = "BCF")
{
Loop, 8
{
a := bcf_2000_%p%_fader_%A_Index%_pick
If (a!=1)
{
Get_midi_for_function(a, bcf_2000_%p%_fader_%A_Index%_preset_pick,  midi_channel,  midi_cc)
preset_output .= BCL_fader_section(BCTYPE, A_Index, function_desc%a%, midi_channel, midi_cc, bcf_2000_%p%_fader_%A_Index%_showvalue)
}
}
}
preset_output .= BCL_Store_Section(BCTYPE,p,%BCTYPE%_Preset_%p%_name)
Return preset_output
}
BCL_Block_start(TYP)
{
r := %TYP%_BCL_Header(True)
If (TYP = "BCF")
r .= str2hex("$rev F1 `; Paddy for Lightroom") . "F7"
Else
r .= str2hex("$rev R1 `; Paddy for Lightroom") . "F7"
Return r
}
BCL_global_section(TYP)
{
r := %TYP%_BCL_Header() . str2hex("$global") . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .midimode U-1") . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .startup last") . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .footsw auto") . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .rxch off") . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .deviceid 1") . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .txinterval 20") . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .deadtime 100") . "F7"
Return r
}
BCL_Preset_start_Section(TYP, preset_number, Preset_name)
{
r := %TYP%_BCL_Header() . str2hex("$preset `; #" . Preset_number) . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .name '" . Preset_name . "'") . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .snapshot off") . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .request on") . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .egroups 4") . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .fkeys off") . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .lock off") . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .tx $C1 $7F") . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .init") . "F7"
Return r
}
BCL_encoder_section(TYP, encoder_number, mapped_function, channel, cc, showvalue,resolution, led_mode)
{
r:= %TYP%_BCL_Header() . str2hex("$encoder " . encoder_number . " `;" . RegExReplace(mapped_function, "[^a-zA-Z0-9_-\s]","_")  ) . "F7"
If (mapped_function !=1)
{
easypar =.easypar CC %channel% %cc% 0 127 absolute
r .= %TYP%_BCL_Header() . str2hex("  " . easypar) . "F7"
If (showvalue)
sv:="on"
Else
sv:="off"
r .= %TYP%_BCL_Header() . str2hex("  .showvalue " . sv) . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .mode " . led_mode) . "F7"
resolution_delta := resolution//2
r1 := resolution
r2 := r1+ resolution_delta
r3 := r2+ resolution_delta
r4 := r3+ resolution_delta
r .= %TYP%_BCL_Header() . str2hex("  .resolution " . r1 . " " . r2 . " " . r3 . " " . r4) . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .default 0") . "F7"
}
Return r
}
BCL_fader_section(TYP, fader_number, mapped_function, channel, cc, showvalue)
{
r:= %TYP%_BCL_Header() . str2hex("$fader " . fader_number . " `;" . RegExReplace(mapped_function, "[^a-zA-Z0-9_-\s]","_") ) . "F7"
If (mapped_function !=1)
{
easypar =.easypar CC %channel% %cc% 0 127 absolute
r .= %TYP%_BCL_Header() . str2hex("  " . easypar) . "F7"
If (showvalue)
sv:="on"
Else
sv:="off"
r .= %TYP%_BCL_Header() . str2hex("  .showvalue " . sv) . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .motor on") . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .override move") . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .keyoverride off") . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .default 0") . "F7"
}
Return r
}
BCL_button_section(TYP, button_number, mapped_function, channel, cc, showvalue, toggle)
{
r:= %TYP%_BCL_Header() . str2hex("$button " . button_number . " `;" . RegExReplace(mapped_function, "[^a-zA-Z0-9_-\s]","_") ) . "F7"
If (mapped_function !=1)
{
If (toggle)
tg:="toggleon"
Else
tg:="toggleoff"
easypar=.easypar CC %channel% %cc% 127 0 %tg%
r .= %TYP%_BCL_Header() . str2hex("  " . easypar) . "F7"
If (showvalue)
sv:="on"
Else
sv:="off"
r .= %TYP%_BCL_Header() . str2hex("  .showvalue " . sv) . "F7"
r .= %TYP%_BCL_Header() . str2hex("  .default 0") . "F7"
}
Return r
}
BCL_Store_Section(TYP, preset_number,preset_name)
{
r:= %TYP%_BCL_Header() . str2hex("$store " . preset_number . " `;" . RegExReplace(Preset_name, "[^a-zA-Z0-9_-\s]","_") ) . "F7"
Return r
}
BCL_end(TYP)
{
r:= %TYP%_BCL_Header() . str2hex("$end `; Paddy for Lightroom") . "F7"
Return r
}
BCF_BCL_header(start = False)
{
r:= "F0002032001420" . bcl_index_count(start)
Return r
}
BCR_BCL_header(start = False)
{
r:= "F0002032001520" . bcl_index_count(start)
Return r
}
BCL_index_count(start=False)
{
global BCL_Index
If start
BCL_Index:= 0
Else
BCL_Index++
MSB_dec:= BCL_Index>>7
lsb_dec:= BCL_index&127
MSB := convert_base(MSB_dec,10,16)
LSB := convert_base(LSB_dec,10,16)
If StrLen(msb) = 1
msb := "0" . msb
If StrLen(lsb) = 1
lsb := "0" . lsb
r:= msb . lsb
Return r
}
