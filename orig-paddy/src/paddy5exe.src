Return
}
If need_slider_handles
read_all_slider_handles()
If midi_debug_mode And !Midi_run
_MidiDebugMessage("#Paddy sending all Midi out slider values ",A_ThisLabel)
this_is_jpg:=is_jpg()
If (old_is_jpg != this_is_jpg)
midi_run:=0
old_is_jpg:=this_is_jpg
this_is_2010:=(process_number()=10)
If (old_is_2010 != this_is_2010)
midi_run:=0
old_is_2010:=this_is_2010
rpanel := check_right_panel_status()
If (wait_if_many_changed)
{
r:=0
Loop, 126
{
If ((slider_%A_Index%_is_mapped_to_midi<1) And not_osc)
Continue
paddy_sl:=A_Index
If fake_zero_test(Paddy_sl)
{
cv_%A_Index%:=0
Continue
}
If get_slider_handle_from_paddy_virtual_slider_number(paddy_sl,edit_handle,rpanel,1)
Continue
ControlGetText, cv,, ahk_id %edit_handle%
StringReplace, cv, cv, ? ,-,
StringReplace, cv,cv, %a_comma%,%a_dot%
StringReplace, cv,cv, %A_Space%,, All
StringReplace, cv,cv, + ,,
real_slider_%A_Index%:=paddy_sl
cv_%A_Index%:=cv
If cv=
Continue
If (midi_memory_%A_Index%=cv)
Continue
Else
{
If (change_memory_%A_Index%<>cv)
midi_wait_timer:=A_TickCount
change_memory_%A_Index%:=cv
m:=midi_memory_%A_Index%
If !master_just_changed(paddy_sl)
r++
}
}
If (r>1)
{
If !midi_wait_timer
{
midi_wait_timer:=A_TickCount
SetTimer,midioutloop, -100
Return
}
If ((A_TickCount - midi_wait_timer) >time_to_wait_if_many_have_changed)
midi_wait_timer:=0
Else
{
SetTimer,midioutloop, -200
Return
}
}
}
Else
{
Loop, 126
{
If ((slider_%A_Index%_is_mapped_to_midi > 0) or use_osc)
{
paddy_sl:=A_Index
If fake_zero_test(Paddy_sl)
{
cv_%A_Index% :=0
real_slider_%A_Index%:=A_Index
Continue
}
If get_slider_handle_from_paddy_virtual_slider_number(paddy_sl,edit_handle,rpanel,1)
Continue
ControlGetText, cv,, ahk_id %edit_handle%
StringReplace, cv, cv, ? ,-,
StringReplace, cv,cv, %a_comma%,%a_dot%
StringReplace, cv,cv, %A_Space%,, All
StringReplace, cv_%A_Index%,cv, + ,,
real_slider_%A_Index%:=paddy_sl
}
}
}
Loop, 126
{
If ((slider_%A_Index%_is_mapped_to_midi > 0) or (use_osc))
{
paddy_sl := real_slider_%A_Index%
original_slider:=A_Index
cv:=cv_%A_Index%
If cv=
Continue
If (midi_run And (midi_memory_%A_Index%=cv) And !(slider_number_%original_slider%_moved_by))
Continue
If (super_sensitivity_mode = 1)
{
midi_out_value:= (cv - super_sensitive_low_%original_slider%)/(super_sensitive_high_%original_slider% - super_sensitive_low_%original_slider%)*127
If (midi_out_value > 127)
midi_out_value := 127
Else If (midi_out_value < 0)
midi_out_value := 0
}
Else
{
If ((paddy_sl = 3) or (paddy_sl=1003))
midi_out_value := Temp_to_midi(cv)
Else
{
If (high_sensitivity_mode = 1)
midi_out_value:= (cv - slider_low_sensitive_%paddy_sl%)*(slider_range_factor_sensitive_%paddy_sl%)
Else
midi_out_value:= (cv - slider_low_%paddy_sl%)*(slider_range_factor_normal_%paddy_sl%)
}
}
If (mapped_to_pitch_%original_slider%)
{
cc :=223 + mapped_to_pitch_%original_slider%
midiOutShortMsg(generic_h_midiout, cc, 0, midi_out_value)
}
If use_osc
{
out:=Round(midi_out_value*(10000/127))
COM_Invoke(XkeysObject,"OSC_send_slider_p",original_slider, out)
}
time_slider_moved_by_midi_out_%original_slider%:=A_TickCount
last_handle:=slider_number_%original_slider%_moved_by
lloop := midi_count_for_paddy_slider_%original_slider%
Loop, %lloop%
{
out_slider := midi_control_%A_Index%_for_paddy_slider_%original_slider%
cc := 175 + midi_channel_%A_Index%_for_paddy_slider_%original_slider%
If (generic_H_midiout And (generic_H_midiin<>last_handle))
midiOutShortMsg(generic_h_midiout, cc, out_slider, midi_out_value)
If (bcr_h_midiout And (bcr_H_midiin<>last_handle))
midiOutShortMsg(bcr_h_midiout, cc, out_slider, midi_out_value)
If (bcF_h_midiout And (bcf_H_midiin<>last_handle))
midiOutShortMsg(bcf_h_midiout, cc, out_slider, midi_out_value)
}
lloop := nocturn_count_for_paddy_slider_%original_slider%
Loop, %lloop%
{
If (nocturn_h_midiout And (nocturn_H_midiin<>last_handle))
midiOutShortMsg(nocturn_h_midiout, 175 + mapped_to_nocturn_channel_%original_slider%_%A_Index%, mapped_to_nocturn_cc_%original_slider%_%A_Index%, midi_out_value)
}
midi_memory_%original_slider% := cv
slider_number_%original_slider%_moved_by:=0
If midi_debug_mode
_MidiDebugMessage(slider_label_%original_slider% . ": old value: " . midi_memory_%original_slider% . "  new value : " . cv . "  new midi value: " . midi_out_value,A_ThisLabel)
}
}
midi_run :=1
SetTimer,midioutloop, -120
Return
}
