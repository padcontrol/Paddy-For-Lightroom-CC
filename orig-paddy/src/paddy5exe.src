execute(keynumber)
{
global
k := key%keynumber%
k1 := skey%keynumber%
f_number := %k%_pick
f:= function_call%f_number%
ifwinactive, ahk_class CameraRaw_WindowClass
camera_raw:=1
else
camera_raw:=0
If !LRACTIVE and !Camera_raw and !(xkeys_always_allowed and (keynumber=99997)) and !((keynumber = 9998) and allow_midi_at_all_times)
{
if (DEBUGMODE)
_Paddyddmessage("LR not active, returning",A_thisfunc)
none(keynumber)
Return
}
If (recorder = 1)
{
if (DEBUGMODE)
_paddyDDMessage("calling recorder, Function number: " . f_number . " key: " . k,a_thisfunc)
record(f_number,%k%_preset_pick,skey%keynumber%)
}
if  !in_lr_develop_fast()
{
ControlGetFocus, currentfocus, Lightroom ahk_class AgWinMainFrame
if  in_lr_library_fast()
{
IfInString, currentfocus, Edit
{
if (VERBOSE_DEBUG)
_PaddyDDMessage("in an edit box: " . currentfocus, a_thisfunc)
if ((f_number<>2628) and (f_number<>2629) and (f_number<>172) and (f_number<>173) and (f_number<>8))
{
none(keynumber)
Return
}
}
}
else
{
If !allowed_in_other_modules_%f_number%
{
none(keynumber)
return
}
}
}
if (DEBUGMODE)
_paddyddmessage("key: " . k . "--->function: " . f . " [" . %k%_preset_pick . "]", a_thisfunc)
Check := SubStr(f,1,3)
If (Check = "SK:") and !camera_raw
{
If in_lr_develop_fast()
{
finish_slider_event()
last_event := 0
}
argument := SubStr(f,4)
if (DEBUGMODE)
_paddyddmessage("sending key >" . argument . "<", a_thisfunc)
Send %argument%
sleep, 20
}
Else IF (Check = "SSU" )
{
argument := SubStr(f,4)
Move_slider(argument, 1, 1,0)
}
Else IF (Check = "SSD" )
{
argument := SubStr(f,4)
Move_slider(argument, 1, 2,0)
}
Else IF (Check = "MSU" )
{
argument := SubStr(f,4)
Move_slider(argument, 2, 1,0)
}
Else IF (Check = "MSD" )
{
argument := SubStr(f,4)
Move_slider(argument, 2, 2,0)
}
Else IF (Check = "LSU" )
{
argument := SubStr(f,4)
Move_slider(argument, 3, 1,0)
}
Else IF (Check = "LSD" )
{
argument := SubStr(f,4)
Move_slider(argument, 3, 2,0)
}
Else IF (Check = "REV" )
{
argument := SubStr(f,4)
Revert_slider(argument)
}
Else IF (Check = "SEL" )
{
argument := SubStr(f,4)
select_slider(argument)
}
Else IF ((Check = "MID" ) or (Check = "_ID" ))
{
Paddymessage2(M_Midi_function_cant_execute_from_key)
Return
}
Else IF (Check = "RES" )
{
argument := SubStr(f,4)
Reset_slider(Argument)
}
Else
{
If (last_event = 1) And ((f_number<1319) or(f_number>1324))
{
finish_slider_event()
last_event = 0
}
SetTitleMatchMode,2
if (DEBUGMODE)
_paddyddmessage("Executing " . f, a_thisfunc)
if camera_raw
{
if	(allowed_in_camera_raw_%f_number%)
%f%(keynumber)
Else
none(keynumber)
return
}
Else
%f%(keynumber)
}
Return
}
