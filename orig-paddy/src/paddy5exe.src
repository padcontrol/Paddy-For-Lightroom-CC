read_osc_settings()
{
global
OSC_command_for_preset_set_1_tab:="/Paddy/Lightroom/preset_set/1"
OSC_command_for_preset_set_2_tab:="/Paddy/Lightroom/preset_set/2"
OSC_command_for_preset_set_3_tab:="/Paddy/Lightroom/preset_set/3"
OSC_command_for_profile_set_1_tab:="/Paddy/Lightroom/camera_tab"
OSC_command_for_profile_set_2_tab:="/Paddy/Lightroom/camera_tab"
OSC_command_for_profile_set_3_tab:="/Paddy/Lightroom/camera_tab"
Loop, 7
{
OSC_command_for_keyword_set_%A_Index%_tab:="/Paddy/Lightroom/keyword_set/" . A_Index
OSC_name_for_keyword_set_%A_Index%_tab:="KW " . A_Index
}
Loop, 3
{
set:=(A_Index-1) * 1000
Loop, 10
{
set++
OSC_preset_%set%_used := 0
OSC_profile_%set%_used := 0
}
}
If !FileExist(paddy_settingsdir . "\Paddy_osc.ini")
Return
Loop, Read, %paddy_settingsdir%\Paddy_osc.ini
{
StringSplit, parameter,A_LoopReadLine,=
%parameter1%:= RTrim(parameter2)
i:=%parameter1%
If (Verbose_debug)
_paddyddmessage(parameter1 . "=" . i,A_ThisFunc)
}
Return
}
write_osc_settings()
{
global
local writefile
writefile:=""
Loop, 3
{
set:=(A_Index-1) * 1000
Loop, 10
{
set++
If !OSC_preset_%set%_used
OSC_preset_%set%_used:=0
writefile.= "OSC_preset_" . set . "_used=" . OSC_preset_%set%_used . "`n"
If (OSC_preset_%set%_used)
{
writefile.= "OSC_preset_" . set "=" . OSC_preset_%set% . "`n"
writefile.= "OSC_preset_" . set "_shortname=" . OSC_preset_%set%_shortname . "`n"
writefile.= "OSC_preset_" . set "_color=" . OSC_preset_%set%_color . "`n"
}
}
}
Loop, 3
{
set:=(A_Index-1) * 1000
Loop, 10
{
set++
If !OSC_profile_%set%_used
OSC_profile_%set%_used:=0
writefile.= "OSC_profile_" . set . "_used=" . OSC_profile_%set%_used . "`n"
If (OSC_profile_%set%_used)
{
writefile.= "OSC_profile_" . set "=" . OSC_profile_%set% . "`n"
writefile.= "OSC_profile_" . set "_shortname=" . OSC_profile_%set%_shortname . "`n"
writefile.= "OSC_profile_" . set "_color=" . OSC_profile_%set%_color . "`n"
}
}
}
Loop, 7
{
set:=(A_Index-1) * 1000
Loop, 70
{
set++
If !OSC_keyword_%set%_used
OSC_keyword_%set%_used:=0
writefile.= "OSC_keyword_" . set . "_used=" . OSC_keyword_%set%_used . "`n"
If (OSC_keyword_%set%_used)
{
writefile.= "OSC_keyword_" . set "=" . OSC_keyword_%set% . "`n"
writefile.= "OSC_keyword_" . set "_label=" . OSC_keyword_%set%_label . "`n"
writefile.= "OSC_keyword_" . set "_color=" . OSC_keyword_%set%_color . "`n"
}
}
}
Loop, 3
{
writefile.= "OSC_command_for_preset_set_" . A_Index . "_tab=" . OSC_command_for_preset_set_%A_Index%_tab . "`n"
writefile.= "OSC_command_for_profile_set_" . A_Index . "_tab=" . OSC_command_for_profile_set_%A_Index%_tab . "`n"
}
Loop, 7
{
writefile.= "OSC_command_for_keyword_set_" . A_Index . "_tab=" . OSC_command_for_keyword_set_%A_Index%_tab . "`n"
writefile.= "OSC_name_for_keyword_set_" . A_Index . "_tab=" . OSC_name_for_keyword_set_%A_Index%_tab . "`n"
}
Loop
{
FileDelete,%paddy_settingsdir%\Paddy_osc.ini
Sleep, 100
If !FileExist(paddy_settingsdir . "\Paddy_osc.ini")
Break
}
FileAppend, %writefile%,%paddy_settingsdir%\Paddy_osc.ini
Return
}
