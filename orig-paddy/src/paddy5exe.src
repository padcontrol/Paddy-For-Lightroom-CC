convert_named_key(key)
{
if (strlen(key)>1)
{
stringreplace, key, key, Bracket_Left, [
stringreplace, key, key, Bracket_Right, ]
stringreplace, key, key, BackSlash, \
stringreplace, key, key, Slash, /
stringreplace, key, key, Equals, =
stringreplace, key, key, Semicolon, `;
stringreplace, key, key, Grave, ``
stringreplace, key, key, Apostrophe, '
stringreplace, key,key, Plus, +
stringreplace, key,key, Hash, #
stringreplace, key,key, Exlmark, !
stringreplace, key,key, Colon, :
stringreplace, key,key, Minus, -
stringreplace, key,key, comma, `,
stringreplace, key,key, Dot, .
stringreplace, key,key, Smaller, <
stringreplace, key,key, spanish_n, ñ
}
return key
}
